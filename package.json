{
  "name": "api-meteorologia",
  "version": "1.0.0",
  "description": "Typescript NodeJS api",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node ./dist/src/index.js",
    "start:dev": "ts-node-dev ./src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts' ",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LGeremia/api-meteorologia.git"
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "API"
  ],
  "author": "Leonardo Geremia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LGeremia/api-meteorologia/issues"
  },
  "homepage": "https://github.com/LGeremia/api-meteorologia#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/babel__core": "^7.1.19",
    "@types/babel__generator": "^7.6.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.40",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "module-alias": "^2.2.2"
  }
}
